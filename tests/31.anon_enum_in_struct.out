@[translated]
module main

// Test for anonymous enums inside structs
// V doesn't support anonymous inline enum types like struct/union,
// so C2V needs to generate a named enum before the struct.
enum With_anon_enum_Status {
	status_ok
	status_error
}

struct With_anon_enum {
	id     int
	status With_anon_enum_Status
}

enum With_anon_enum_values_Color {
	color_red   = 1
	color_green = 2
	color_blue  = 4
}

struct With_anon_enum_values {
	color     With_anon_enum_values_Color
	intensity int
}
